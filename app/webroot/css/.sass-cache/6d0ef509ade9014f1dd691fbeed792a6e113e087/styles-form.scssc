@linei:@has_childrenT:@template"`%/* YOU CAN PROBABLY DELETE THESE */
body {
    font-size:14px;
    margin:0;
    padding:0;
}
div.cf {
    clear:both;
}
ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
}

/* =============== standards =============== */
$project:#ff6600;
$person:#00b9db;
$org:#7bd900;
$event:#d200ff;

$bg:#2a2f33;
$dark-gray:#1c1c1c;
$light-gray:#BABABA;
$neutral:#f3bc00;

$current-type:$project;

@mixin field {
	background-color:darken($bg, 3%);
    border:1px lighten($bg, 5%) solid;
    border-radius: 5px;
    box-shadow: inset 0 0 5px darken($bg,25%);
    color:$current-type;
    font-size:14px;
    margin-bottom:10px;
    padding:10px;
    width:340px;
}
@mixin field-focus {
	background-color:$bg;
    border:1px $current-type solid;
    outline:0!important;
}
@mixin form-button {
	background: #cacccb;
	background: -moz-linear-gradient(top,  #cacccb 0%, #cacbc6 3%, #c9c9c9 6%, #c8cac9 13%, #c2c2c2 26%, #bcc0bf 39%, #b8b9bb 48%, #b9b9bb 52%, #b7b5b6 55%, #b5b5b7 58%, #adafae 71%, #adaeb0 74%, #a9adae 77%, #aaaaac 81%, #a6a6a8 97%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#cacccb), color-stop(3%,#cacbc6), color-stop(6%,#c9c9c9), color-stop(13%,#c8cac9), color-stop(26%,#c2c2c2), color-stop(39%,#bcc0bf), color-stop(48%,#b8b9bb), color-stop(52%,#b9b9bb), color-stop(55%,#b7b5b6), color-stop(58%,#b5b5b7), color-stop(71%,#adafae), color-stop(74%,#adaeb0), color-stop(77%,#a9adae), color-stop(81%,#aaaaac), color-stop(97%,#a6a6a8));
	background: -webkit-linear-gradient(top,  #cacccb 0%,#cacbc6 3%,#c9c9c9 6%,#c8cac9 13%,#c2c2c2 26%,#bcc0bf 39%,#b8b9bb 48%,#b9b9bb 52%,#b7b5b6 55%,#b5b5b7 58%,#adafae 71%,#adaeb0 74%,#a9adae 77%,#aaaaac 81%,#a6a6a8 97%);
	background: -o-linear-gradient(top,  #cacccb 0%,#cacbc6 3%,#c9c9c9 6%,#c8cac9 13%,#c2c2c2 26%,#bcc0bf 39%,#b8b9bb 48%,#b9b9bb 52%,#b7b5b6 55%,#b5b5b7 58%,#adafae 71%,#adaeb0 74%,#a9adae 77%,#aaaaac 81%,#a6a6a8 97%);
	background: -ms-linear-gradient(top,  #cacccb 0%,#cacbc6 3%,#c9c9c9 6%,#c8cac9 13%,#c2c2c2 26%,#bcc0bf 39%,#b8b9bb 48%,#b9b9bb 52%,#b7b5b6 55%,#b5b5b7 58%,#adafae 71%,#adaeb0 74%,#a9adae 77%,#aaaaac 81%,#a6a6a8 97%);
	background: linear-gradient(top,  #cacccb 0%,#cacbc6 3%,#c9c9c9 6%,#c8cac9 13%,#c2c2c2 26%,#bcc0bf 39%,#b8b9bb 48%,#b9b9bb 52%,#b7b5b6 55%,#b5b5b7 58%,#adafae 71%,#adaeb0 74%,#a9adae 77%,#aaaaac 81%,#a6a6a8 97%);
	border:1px $bg solid;
	border-radius:5px;
	box-shadow: 0 0 2px darken($bg,15%);
	color: $bg;
	font-family:Gotham, Helvetica, Arial, sans-serif;
	font-size:12px;
	font-weight:600;
	text-transform:uppercase;
	padding:7px 15px;
	&:hover {
		color:darken($current-type,5%);
	}
}

/* =============== forms EDIT =============== */
li.story-save {
	button {
		@include form-button;
	}
}

/* =============== forms NEW =============== */
div.form-container {
    font-family:Helvetica, sans-serif;
    margin:0 auto;
    width:780px;
    form {
        div.section-in-form {
            border-bottom:1px darken($light-gray,45%) solid;
            padding:20px 0;
            &:last-child {
                border:0;
            }
        }
        div.stub {
            ul.required-fields {
                select.entry-type-select {
                    margin-bottom:23px;
                    width:340px;
                }
            }
        }
        div.basics {
            ul.basic-fields {
                li.entry-source {
                    color:lighten($bg, 40%);
                    margin-bottom:10px;
                    position:relative;
                    top:-5px;
                }
            }
            ul.basic-labels {
                li.label-entry-date {
                    margin-top:200px;
                }
            }
        }
    }
    div.section-in-form {
        border-bottom:1px darken($light-gray,45%) solid;
        padding:20px 0;
        &:last-child {
            border:0;
        }
    }
    div.stories-col {
        div.stories {
            div.story-collapsed {display:none;}
            p.story-collapsed-heading {
                background:darken($bg, 5%);
                border:1px lighten($bg, 10%) solid;
                border-radius:5px;
				color:$current-type;
				font-family:Gotham, Helvetica, Arial, sans-serif;
				font-size:16px;
                font-weight:600;
				text-transform:uppercase;
                padding:10px;
                width:100%;
                
                span.info {
                    color:lighten($bg, 5%);
                    float:right;
                    font-size:12px;
                    font-weight:normal;
                    text-shadow:none;
                }
            }
        }
        ul.story-explanation {
            li.label-story-header{
                float:left;
                width:20%;
            }
            li.story-helpertext {
                float:left;
                width:440px;
            }
        }
        ul.story-labels {
            li.label-story-header {
                margin-bottom:117px;
            }
            li.label-story-story {
                margin-bottom:150px;
            }
        }
        ul.story-fields {
            li.story-helpertext {
                width:440px
            }
            li.story-media {
                height:45px;
            }
            li.story-source {
                color:lighten($bg, 40%);
                margin-bottom:10px;
                position:relative;
                top:-5px;
            }
        }
    }
    div.buttons {
        ul.story-or-connection {
            float:left;
            margin:15px 0 0 20%;
            padding:0;
            width:440px;
            a {
                background-repeat:no-repeat;
                color:$light-gray;
                display:block;
                float:left;
                height:70px;
                margin-right:10px;
                p {
                    margin-top:45px;
                    text-align:center;
                }
                &.btn-story {
                    background-image:url('../img/story_icon.png');
                    background-position:15px 0px;
                    border-right:1px $light-gray solid;
                    padding-right:20px;
                    width:100px;
                    &:hover {
                        background-image:url('../img/story_icon_yellow.png');
                    }
                }
                &.btn-connect {
                    background-image:url('../img/connection_icon.png');
                    background-position:52px 0px;
                    padding-left:20px;
                    &:hover {
                        background-image:url('../img/connection_icon_yellow.png');
                    }
                }
            }
            li.share-helpertext {
                margin-bottom:30px;
            }
        }
    }
	h1 {
		text-transform:uppercase;
	}
	h2 {
		font-family:Gotham, Helvetica, sans-serif;
		padding:5px 0 0 0;
		text-transform:uppercase;
	}
	input[type="submit"] {
		@include form-button;
	}
    li.source-title, li.source-url {
        display:none;
		input {
			@include field;
			&:focus {
                @include field-focus;
            }
		}
        label {
            color:lighten($light-gray, 10%);
            margin-right:10px;
            text-shadow:1px 1px 1px #000;
        }
    }
	li.source-url input {
		width:275px;
	}
    p {
        line-height:20px;
        text-shadow:1px 1px 1px #000;
        &.section-header {
            color:$neutral;
            font-family:"Gotham", Helvetica, sans-serif;
            font-size:17px;
            font-weight:700;
            text-transform:uppercase;
        }
    }
    span.required {
        color:$neutral;
    }
    span.helper-popups {
        background:darken($bg, 2%);
        border:1px darken($current-type, 20%) solid;
        border-radius:5px;
        display:none;
        font-size:12px;
        margin-left:10px;
        padding:10px;
        position:absolute;
        text-shadow:1px 1px 1px #000;
        &#helper-date {
            float:right;
            width:250px;
        }
        &#helper-descript {
            float:right;
            width:150px;
        }
        &#helper-story-author {
            width:250px;
        }
        &#helper-story-story {
            width:150px;
        }
    }
    ul.all-labels {
        color:lighten($bg, 65%);
        float:left;
        font-size:14px;
        font-weight:500;
        text-shadow:1px 1px 1px #000;
        width:20%;

        li {
            margin-bottom:33px;
        }
    }
    ul.all-fields {
        float:right;
        width:80%;
		li.fields {
			input[type="text"], textarea {
	            @include field;
	            &:focus {
	                @include field-focus;
	            }
	        }
	        textarea {
	            height:150px;
	            width:440px;
	        }
		}
    }
}

/* =============== token =============== */
ul.token-input-list-hcg {
	@include field;
	background-color:darken($bg, 3%)!important;
    border:1px lighten($bg, 5%) solid!important;
	font-family:Helvetica, Arial, sans-serif!important;
	padding:5px!important;
	width:350px!important;
	li.token-input-input-token-hcg { //new tag
		border-radius:5px;
		input {
			background-color:$bg!important;
			margin-bottom:0!important;
			padding:5px!important;
		}
	}
	li.token-input-token-hcg {  //selected tag
		background-color:$bg!important;
		border:0!important;
		border-radius:5px;
		font-size:14px;
		margin-bottom:5px!important;
		padding:5px;
	}
}











